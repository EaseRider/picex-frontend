{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "plugins": ["react", "jsx", "@tanstack/query"],
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["@mui/*/*/*"]
      }
    ]
  },
  "extends": ["plugin:@tanstack/eslint-plugin-query/recommended"]
}
